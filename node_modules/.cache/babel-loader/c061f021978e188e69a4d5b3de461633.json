A;AAQA,KApBA;AAqBA,IAAA,aArBA,yBAqBA,MArBA,EAqBA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,kBACA,MADA,aACA;AAAA,QAAA,MAAA,EAAA,KAAA,YAAA,CAAA;AAAA,OADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,iBAAA,MAAA;AAAA,SAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KA9BA;AAgCA,IAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CACA,QADA,EACA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,OADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,cAAA;;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OARA;AASA;AA1CA,GAXA;AAuDA,EAAA,QAAA,oBACA,UAAA,CAAA;AACA,IAAA,QAAA,EAAA,gBADA;AAEA,IAAA,YAAA,EAAA;AAFA,GAAA,CADA;AAvDA,CAAA","sourcesContent":["<template>\n  <div class=\"groupSelect\">\n    <v-item-group>\n      <v-container>\n        <v-row>\n          <v-col>\n            <v-item v-slot:default=\"{ active, toggle }\">\n              <v-card\n                :color=\"active ? 'primary' : ''\"\n                class=\"d-flex align-center\"\n                dark\n                height=\"60\"\n                @click=\"toggle\"\n              >\n                <div class=\"flex-grow-1 text-center\">\n                  <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n                    <template v-slot:activator=\"{ on, attrs }\">\n                      <div color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">Open Room</div>\n                    </template>\n                    <v-card>\n                      <v-card-title>\n                        <span class=\"headline\">Add A Chat Room</span>\n                      </v-card-title>\n                      <v-card-text>\n                        <v-container>\n                          <v-row>\n                            <v-col cols=\"12\">\n                              <v-text-field v-model=\"roomName\" label=\"Room Name\" required></v-text-field>\n                            </v-col>\n                          </v-row>\n                        </v-container>\n                      </v-card-text>\n                      <v-card-actions>\n                        <v-spacer></v-spacer>\n                        <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Cancel</v-btn>\n                        <v-btn color=\"blue darken-1\" text @click=\"addRoom()\">Save</v-btn>\n                      </v-card-actions>\n                    </v-card>\n                  </v-dialog>\n                </div>\n              </v-card>\n            </v-item>\n          </v-col>\n        </v-row>\n        <v-row>\n          <v-col v-for=\"btn in allRooms\" :key=\"btn.id\" cols=\"12\" md=\"4\">\n            <v-item v-slot:default=\"{ active, toggle }\">\n              <v-card\n                v-on:click=\"roomNameClick(btn.id)\"\n                :color=\"active ? 'primary' : ''\"\n                class=\"d-flex align-center\"\n                dark\n                height=\"60\"\n                @click=\"toggle\"\n              >\n                <div class=\"flex-grow-1 text-center\">{{btn.name}}</div>\n              </v-card>\n            </v-item>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-item-group>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nconst axios = require(\"axios\");\nimport store from \"../store\";\n\nexport default {\n  name: \"GroupSelect\",\n  data() {\n    return {\n      dialog: false,\n      roomName: \"\",\n    };\n  },\n  mounted() {\n    this.poll(this.fetchGroupList);\n  },\n  methods: {\n    poll: async function (cb) {\n      let id = setTimeout(() => {\n        cb()\n          .then(this.poll(cb))\n          .catch(() => {\n            console.error(\"somthing went wrong with polling\");\n          });\n      }, 1000);\n      this.timeoutIds.push(id);\n    },\n    fetchGroupList: function () {\n      return axios\n        .get(\"/rooms\")\n        .then(function (response) {\n          store.dispatch(\"rooms/setRooms\", response.data);\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n    roomNameClick(RoomId) {\n      axios\n        .post(`/rooms/${RoomId}/users`, { userId: this.loggedinUser.id })\n        .then(() => {\n          this.$router.push({ path: `chat/${RoomId}` });\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n\n    addRoom: function () {\n      axios\n        .post(\"/rooms\", { name: this.roomName })\n        .then(() => {\n          this.fetchGroupList();\n          this.dialog = false;\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n  },\n  computed: {\n    ...mapGetters({\n      allRooms: \"rooms/roomList\",\n      loggedinUser: \"rooms/loggedinUser\",\n    }),\n  },\n};\n</script>\n\n<style scoped>\n</style>\n"],"sourceRoot":"src/views"}]}